{
  "name": "soft-sight",
  "description": "Retrieve list of likely explicit followers",
  "version": "0.0.0",
  "author": {
    "name": "Mark Hernandez",
    "email": "mark@lion-byte.com",
    "url": "https://www.lion-byte.com/"
  },
  "main": "n/a",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "build:lambda": "netlify-lambda build src/lambdas",
    "dev": "gatsby develop",
    "dev:lambda": "netlify-lambda serve src/lambdas",
    "format": "prettier **/*.js --write --ignore-path .gitignore && standard --fix",
    "format:styles": "prettier src/styles/*.less --write",
    "format:docs": "prettier *.md --write",
    "report": "nyc report --reporter=lcov",
    "test": "standard && nyc mocha"
  },
  "proxy": {
    "/.netlify/functions": {
      "target": "http://localhost:9000",
      "pathRewrite": {
        "^/\\.netlify/functions": ""
      }
    }
  },
  "standard": {
    "globals": [
      "after",
      "afterEach",
      "before",
      "beforeEach",
      "describe",
      "graphql",
      "it"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": [
        "lint-staged"
      ]
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "standard --fix",
      "git add"
    ],
    "*.{md,less}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "husky": "^0.15.0-rc.13",
    "lint-staged": "^7.0.1",
    "mocha": "^5.0.5",
    "nyc": "^11.6.0",
    "prettier": "^1.11.1",
    "standard": "^11.0.1"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "dotenv": "^4.0.0",
    "gatsby": "^1.9.243",
    "gatsby-link": "^1.6.40",
    "gatsby-plugin-catch-links": "^1.0.19",
    "gatsby-plugin-less": "^1.1.8",
    "gatsby-plugin-netlify": "^1.0.19",
    "gatsby-plugin-react-helmet": "^2.0.8",
    "gatsby-plugin-react-next": "^1.0.11",
    "milligram": "^1.3.0",
    "moment": "^2.22.0",
    "netlify-lambda": "^0.3.0",
    "passport": "^0.4.0",
    "passport-tumblr": "^0.1.2",
    "react-helmet": "^5.2.0",
    "tumblr.js": "^1.1.1"
  }
}
